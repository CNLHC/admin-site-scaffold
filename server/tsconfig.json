{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": ["node_modules"],
  "include": ["index.d.ts", "next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
